                <div class="card">
                <div class="bdr">
                <section>
                    <table class="table table-striped">
                        <thead>
                            <tr>
                            <th>Subject</th>
                            <th>Category</th>
                            <th>Writer</th>
                            <th>Date</th>
                            <th>Start Time</th>
                            <th>End Time</th>
                            <th>Duration</th>
                            <th></th>
                            </tr>
                        </thead>
                    
                        <tbody>
                            <%if(data.length===0){%>
                                <tr>
                                    <td colspan="5">No data</td>
                                </tr>
                            <%}else{for(let i=0;i<data.length;i++){%>
                                <tr>
                                    <td>Subject</td>
                                    <td><%=data[i].department%></td>
                                    <td>Writer</td>
                                    <td><%=data[i].date%></td>
                                    <td><%=data[i].start%></td>
                                    <td><%=data[i].end%></td>
                                    <td>2 hours</td>
                                    <%if(enroll[i]===false){%>
                                    <td><button class="btn btn-success btn-block" ><a href="/students/testEnroll/<%=data[i].s_code%>$<%=data[i].t_code%>">Register</a></button></td>
                                    <%}else{%>
                                        <td>Registered</td>
                                    <%}%>
                                    <!-- <td><button class="btn btn-danger btn-block"><a href="/tests/deleteSubject/<%=data[i].s_code%>">Delete</a></button></td>
                                    <td><button class="btn btn-secondary btn-block"><a href="#">Edit</a></button></td> -->
                                </tr>
                            <%}}%>
                            
